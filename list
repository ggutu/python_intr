
lucky_numbers = [4,8,15,16,23,24]
friends = ["kevin","Kena", "karen", "jim","Gutu", "mark"]
friends.extend(lucky_numbers)
print(friends)
output
['kevin', 'Kena', 'karen', 'jim', 'Gutu', 'mark', 4, 8, 15, 16, 23, 24]
====
append
lucky_numbers = [4,8,15,16,23,24]
friends = ["kevin","Kena", "karen", "jim","Gutu", "mark"]
friends.append("Gutu")
print(friends)
output
['kevin', 'Kena', 'karen', 'jim', 'Gutu', 'mark', 'Gutu']
